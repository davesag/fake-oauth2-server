{
  "name": "fake-oauth2-server",
  "version": "0.0.2",
  "description": "An OAuth2 server implementation to be used for testing",
  "main": "src/server.js",
  "engines": {
    "node": "10.4.1",
    "npm": "6.1.0"
  },
  "scripts": {
    "eslint-check": "eslint --print-config .eslintrc.js | eslint-config-prettier-check",
    "lint": "eslint .",
    "start": "NODE_PATH=. node src/server.js",
    "precommit": "lint-staged",
    "prettier": "prettier --single-quote --no-semi --write '**/*.{js,json}'",
    "test": "NODE_ENV=test NODE_PATH=. jasmine",
    "test:coverage": "NODE_ENV=test NODE_PATH=. nyc jasmine"
  },
  "author": "Bence Eros <ebence88@gmail.com>",
  "contributors": [
    "Dave Sag <davesag@gmail.com>"
  ],
  "keywords": [
    "oAuth",
    "oAuth2",
    "mock",
    "fake",
    "mock-server"
  ],
  "lint-staged": {
    "**/*.{js,json}": [
      "prettier --single-quote --no-semi --write",
      "git add"
    ]
  },
  "nyc": {
    "check-coverage": true,
    "per-file": true,
    "lines": 64,
    "statements": 65,
    "functions": 61,
    "branches": 61,
    "include": [
      "src/**/*.js"
    ],
    "exclude": [
      "src/server.js"
    ],
    "reporter": [
      "lcov",
      "text"
    ],
    "all": true,
    "cache": true
  },
  "license": "ISC",
  "repository": "https://github.com/patientsknowbest/fake-oauth2-server.git",
  "bugs": {
    "url": "https://github.com/patientsknowbest/fake-oauth2-server/issues"
  },
  "homepage": "https://github.com/patientsknowbest/fake-oauth2-server#readme",
  "dependencies": {
    "body-parser": "^1.18.3",
    "express": "^4.16.3",
    "express-session": "^1.15.6",
    "morgan": "^1.9.0",
    "randomstring": "^1.1.5",
    "underscore": "^1.9.1"
  },
  "devDependencies": {
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^2.8.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jasmine": "^2.10.1",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-prettier": "^2.3.1",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-standard": "^3.1.0",
    "husky": "^0.14.3",
    "istanbul": "^0.4.5",
    "jasmine": "^3.1.0",
    "lint-staged": "^7.1.3",
    "node-fetch": "^2.1.2",
    "node-mocks-http": "^1.7.0",
    "nyc": "^12.0.2",
    "prettier": "^1.13.5"
  }
}
